<h1 class="display-4 text-center">Docentes</h1>
<hr>
<div class="container py-4">
			<table id="gtabla" class="table table-striped table-bordered   ">
			<thead >
				<tr class="bg-danger text-white ">
					<th scope="col">id</th>
					<th scope="col">Nombre</th>
					<th scope="col">edad</th>
					<th scope="col">Correo</th>
					<th scope="col">Opciones</th>

				</tr>
			</thead>
			<tbody>
				{{#each todosdocentes}}
				<tr>
					<th scope="col">{{id_docente}}</th>
					<td>{{nombre_docen}}</td>
					<td>{{edad}}</td>
					<td>{{correo}}</td>
					<td>
						<a href="delete/{{id_docente}}" class="btn btn-danger">Eliminar</a>
						<a href="modifica/{{id_docente}}" class="btn btn-info">Editar</a>
					</td>	
				</tr>
				{{/each}}
			</tbody>
		</table>
</div>